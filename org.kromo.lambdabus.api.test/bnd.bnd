Bundle-Name:    LambdaBus API integration tests

# no tests
-nojunit:       true

-buildpath: \
    org.kromo.lambdabus.api,\
    org.kromo.lambdabus.test,\
    osgi.annotation,\
    slf4j.api,\
    ${junit5-api},\
    ${junit5-deps}

# no tests source folder as this bundle holds tests / contracts to be validated
# in othe bundles
-fixupmessages.test.sources: \
    "folder 'src*test*java' * path"
